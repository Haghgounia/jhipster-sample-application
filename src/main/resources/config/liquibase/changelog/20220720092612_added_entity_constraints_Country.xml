<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Country.
    -->
    <changeSet id="20220720092612-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="continent_id"
                                 baseTableName="country"
                                 constraintName="fk_country__continent_id"
                                 referencedColumnNames="id"
                                 referencedTableName="continent"/>

        <addForeignKeyConstraint baseColumnNames="currency_id"
                                 baseTableName="country"
                                 constraintName="fk_country__currency_id"
                                 referencedColumnNames="id"
                                 referencedTableName="currency"/>

        <addForeignKeyConstraint baseColumnNames="official_language_id"
                                 baseTableName="country"
                                 constraintName="fk_country__official_lan_a1_id"
                                 referencedColumnNames="id"
                                 referencedTableName="language"/>
    </changeSet>
</databaseChangeLog>
